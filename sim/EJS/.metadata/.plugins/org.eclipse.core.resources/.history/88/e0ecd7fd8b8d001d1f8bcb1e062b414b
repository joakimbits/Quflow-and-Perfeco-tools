
import java.util.*;

/** A DrawingIconSelectionEvent is created whenever a user
    clicks on a drawing icon in the palette. */

public class DrawingIconSelectionEvent extends EventObject {

  /** constructs the DrawingIconSelection event with the
      specified source as the source of the event. */

  public DrawingIconSelectionEvent (DrawingIcon source) {
    super (source);
  }

  /** returns the icon that was the source of the event. */
  public DrawingIcon getIcon () {
    return (DrawingIcon)source;
  }

}
